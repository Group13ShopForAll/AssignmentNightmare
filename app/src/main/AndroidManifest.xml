<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="my.edu.utar.assignmentnightmare">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_logo_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AssignmentNightmare">
        <activity
            android:name=".RazorpayPayment"
            android:exported="true" />
        <activity
            android:name=".Voucher"
            android:exported="true" />
        <activity
            android:name=".Checkout"
            android:exported="true" />
        <activity
            android:name=".ShippingFee"
            android:exported="true" />
        <activity android:name=".UpdateProductActivity" />
        <activity android:name=".MyProductActivity" />
        <activity android:name=".PersonalInfoActivity" />
        <activity
            android:name=".DisplayOrder"
            android:exported="true" />
        <activity android:name=".UpdateProfileActivity" />
        <activity android:name=".RegisterAccountPage" />
        <activity android:name=".OrderActivity" />
        <activity android:name=".ProductActivity" />
        <activity android:name=".UploadActivity" />
        <activity android:name=".Profile" />
        <activity android:name=".orderlist" />
        <activity android:name=".Homepagee" />
        <activity android:name=".LoginPage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ShoppingCart" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <service
            android:name=".MyPushService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.huawei.push.action.MESSAGING_EVENT" />
            </intent-filter>
        </service>

    </application>
</manifest>

